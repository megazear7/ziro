{
  "name": "ziro-components",
  "version": "1.0.23",
  "description": "A library of web components",
  "main": "src/ziro-components.js",
  "scripts": {
    "start": "es-dev-server --node-resolve --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rollup --config",
    "watch": "rollup --config --watch",
    "docs": "npm run watch & npm run docs:serve",
    "docs:build": "npm run build && orison build --srcDir docs --buildDir built-docs",
    "docs:serve": "npm run watch & orison serve --srcDir docs",
    "docs:static": "orison static --srcDir docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/megazear7/ziro.git"
  },
  "keywords": [
    "Web",
    "Components"
  ],
  "author": "Alex Lockhart",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/megazear7/ziro/issues"
  },
  "homepage": "https://github.com/megazear7/ziro#readme",
  "dependencies": {
    "es-dev-server": "^2.1.0",
    "orison": "^1.3.4"
  },
  "devDependencies": {
    "rollup": "^2.44.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2"
  }
}
